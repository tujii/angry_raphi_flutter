{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "angryraphi",
          "appId": "1:733842826620:android:c35cdb0ab78268cdcf70f9",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "angryraphi",
          "appId": "1:733842826620:ios:8e29f5acaedeb0edcf70f9",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "angryraphi",
          "configurations": {
            "android": "1:733842826620:android:c35cdb0ab78268cdcf70f9",
            "ios": "1:733842826620:ios:8e29f5acaedeb0edcf70f9",
            "web": "1:733842826620:web:1297526ba92a4820cf70f9",
            "windows": "1:733842826620:web:2587dd68732ee212cf70f9"
          }
        }
      }
    }
  }
}